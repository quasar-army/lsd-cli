{
  "author": "luke@ldiebold.com",
  "repository": "https://github.com/quasar-army/blue-cli",
  "name": "@quasar-army/blue-cli",
  "homepage": "https://github.com/quasar-army/blue-cli",
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "blue": "pnpm build && node ./bin/run.js",
    "release": "pnpm build && bumpp --tag '@quasar-army/blue-cli@v%s' && pnpm publish",
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "pnpm lint",
    "prepack": "pnpm build && oclif manifest",
    "prepare": "pnpm build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "version": "0.0.15",
  "bugs": "https://github.com/quasar-army/blue-cli/issues",
  "keywords": [
    "oclif"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "bin": {
    "blue": "./bin/run.js"
  },
  "dependencies": {
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^4",
    "chalk": "^5.3.0",
    "cosmiconfig": "^9.0.0",
    "dotenv": "^16.3.1",
    "esno": "^4.0.0",
    "laravel-echo": "^1.15.3",
    "pusher-js": "8.4.0-rc2",
    "readline": "^1.3.0",
    "wretch": "^2.8.0"
  },
  "description": "blue project cli",
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.4.1",
    "bumpp": "^9.2.1",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.0.2",
    "eslint-plugin-promise": "^6.1.1",
    "fs-extra": "^11.2.0",
    "mocha": "^10",
    "oclif": "^4.1.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "unbuild": "^2.0.0"
  },
  "engines": {
    "node": "^18 || ^20"
  },
  "license": "MIT",
  "oclif": {
    "bin": "blue",
    "dirname": "blue",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  }
}
